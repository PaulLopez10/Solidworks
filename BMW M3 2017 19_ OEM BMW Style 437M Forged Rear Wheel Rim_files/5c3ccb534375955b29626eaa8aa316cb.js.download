            if (viewportSize.width < (width + gap.horizontal)) {
                width = viewportSize.width - gap.horizontal;
            }
            this.el.setStyle({
                width: width -
                    parseInt(this.el.getStyle('paddingLeft')) -
                    parseInt(this.el.getStyle('paddingRight')) + 'px',
                left: '50%',
                marginLeft: - width / 2 + 'px'
