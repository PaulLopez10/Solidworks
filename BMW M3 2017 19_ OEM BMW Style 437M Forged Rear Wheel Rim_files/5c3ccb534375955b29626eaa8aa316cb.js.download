        hide: function(id) {
            var el = $(id);

            if (el.up('.easybanner-lightbox-el')) {
                popupObject = _lightbox;
            } else if (el.up('.easybanner-awesomebar-el')) {
                popupObject = _awesomebar;
            } else {
                return;
            }

