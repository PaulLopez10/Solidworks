    for (var i in classMapping) {
        if ('accordion' === mode) {
            renameFrom = i;
            renameTo   = classMapping[i];
        } else {
            renameFrom = classMapping[i];
            renameTo   = i;
        }
        menu.select('.' + renameFrom).each(function(el) {
            el.removeClassName(renameFrom);
            el.addClassName(renameTo);
